void DEBUG_API::reboot_cause_fatal_error(){

	print_error(debug_api.DEBUG_STRINGS[debug_api.ERROR_INDEX],
			debug_api.DEBUG_CODES[debug_api.ERROR_INDEX],
			debug_api.DEBUG_LED_CODES[debug_api.ERROR_INDEX]);
	reset_device();
}

void DEBUG_API::reboot_cause_threshold_exeeded(){

	reset_device();
}

