void PACKET_PARSER::parse(){

	assign_run_variables();

	switch(packet_type){

		case PACKET_INFO: // Always run.
		case ROUTER_STATUS:

		case ROUTER_NMAP:

		case ROUTER_ACK:

		case SENSOR_EN:

		case ERROR_MSG:

		case SENSOR_CONFIG:

		case SENSOR_DATA:

		default:
			break;
	}
}
