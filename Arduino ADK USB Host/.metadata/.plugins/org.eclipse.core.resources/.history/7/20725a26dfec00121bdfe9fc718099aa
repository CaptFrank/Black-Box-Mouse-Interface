void DEBUG_API::reboot_cause_error(byte cause){

	print_error(debug_api.DEBUG_STRINGS[cause],
			debug_api.DEBUG_CODES[cause],
			debug_api.DEBUG_LED_CODES[cause],
			"[%s]: Rebooting Device !!! ERR: %d, LED: %h");
	reset_device();
}

